#include <emscripten.h>

EMSCRIPTEN_KEEPALIVE
int getFrequency(int key) {
    switch (key) {
        case 65: return 261; // A → C
        case 83: return 293; // S → D
        case 68: return 329; // D → E
        case 70: return 349; // F → F
        case 71: return 392; // G → G
        case 72: return 440; // H → A
        case 74: return 493; // J → B
        default: return 0;
    }
}
